<Page x:Class="Discord.Pages.UserProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Discord.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UserProfilePage">

    <Grid>
        <DockPanel x:Name="DockPanelMain" x:FieldModifier="private" Background="{DynamicResource Light3_Background}">
            <DockPanel LastChildFill="False">
                <Label Content="MY ACCOUNT" DockPanel.Dock="Top" FontWeight="Bold" Foreground="{DynamicResource Light2_Foregound}"/>
                <Frame x:Name="FrameUserInfo" x:FieldModifier="private" DockPanel.Dock="Top" Navigating="FrameUserInfo_Navigating"/>
            </DockPanel>
        </DockPanel>
    </Grid>
</Page>
