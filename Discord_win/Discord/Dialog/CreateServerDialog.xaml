<Window x:Class="Discord.Dialog.CreateServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord.Dialog"
        mc:Ignorable="d"
        Title="Create a server" Height="138" Width="379" Closing="Window_Closing">
    <Grid>
        <Label Content="Server name" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox x:Name="TextBoxServerName" Height="23" Width="260" Margin="91,13,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" TextWrapping="Wrap"/>
        <Button x:Name="buttonCreate" Content="Create" Width="75" Margin="276,56,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"  Click="ButtonCreate_Click"/>
    </Grid>
</Window>
