<Window x:Class="Discord.Dialog.JoinServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord.Dialog"
        mc:Ignorable="d"
        Title="Join a server" Height="155" Width="346" Closing="Window_Closing">
    <Grid>
        <Label Content="Enter an Instant invite" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox x:Name="TextBoxInstantInvite" Height="23" Width="313" Margin="15,41,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" TextWrapping="Wrap"/>
        <Button x:Name="buttonJoin" Content="Join" Width="75" Margin="253,69,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Click="ButtonJoin_Click"/>
    </Grid>
</Window>
