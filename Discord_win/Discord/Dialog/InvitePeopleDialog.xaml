<Window x:Class="Discord.Dialog.InvitePeopleDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord.Dialog"
        mc:Ignorable="d"
        Height="142" Width="422" Closing="Window_Closing">
    <Grid>
        <Label Content="Instant invite" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox x:Name="TextBoxInstantInvite" x:FieldModifier="private" Height="23" Width="280" Margin="96,14,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" IsReadOnly="True" TextWrapping="Wrap"/>
    </Grid>
</Window>
