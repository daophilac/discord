<Window x:Class="Discord.Dialog.CreateChannelDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord.Dialog"
        mc:Ignorable="d"
        Title="CreateChannelDialog" Height="141" Width="322" Closing="Window_Closing">
    <Grid>
        <Label Content="Channel name" Height="24" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox x:Name="TextBoxChannelName" Height="24" Width="294" Margin="10,39,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button x:Name="ButtonOK" Content="OK" Height="24" Width="48" Margin="0,68,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Click="ButtonOK_Click"/>
        <Label x:Name="LabelError" Height="24" Margin="10,68,63,54" Foreground="Red"/>
    </Grid>
</Window>
