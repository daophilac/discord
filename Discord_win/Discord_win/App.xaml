<Application x:Class="Discord_win.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Discord_win"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Localizable/Notification.xaml"/>
                <ResourceDictionary Source="Resources/Localizable/ExceptionMessage.xaml"/>
                <ResourceDictionary Source="Resources/UiDecoration/ConstraintLayout.xaml"/>
                <ResourceDictionary Source="Resources/UiDecoration/InterfaceDark.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
