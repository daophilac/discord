<Window x:Class="Discord_win.Dialog.JoinServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord_win.Dialog"
        mc:Ignorable="d"
        Title="Join a server" Height="155" Width="276" Closing="Window_Closing">
    <Grid>
        <Label Content="Enter an Instant invite" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxInstantInvite" HorizontalAlignment="Left" Height="23" Margin="15,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230"/>
        <Button x:Name="buttonJoin" Content="Join" HorizontalAlignment="Left" Margin="170,69,0,0" VerticalAlignment="Top" Width="75" Click="ButtonJoin_Click"/>

    </Grid>
</Window>
