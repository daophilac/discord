<Window x:Class="Discord.Dialog.JoinServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord.Dialog"
        mc:Ignorable="d"
        Title="Join a server" Height="155" Width="346" Closing="Window_Closing">
    <Grid>
        <Label Content="Enter an Instant invite" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxInstantInvite" HorizontalAlignment="Left" Height="23" Margin="15,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="313"/>
        <Button x:Name="buttonJoin" Content="Join" HorizontalAlignment="Left" Margin="253,69,0,0" VerticalAlignment="Top" Width="75" Click="ButtonJoin_Click"/>

    </Grid>
</Window>
