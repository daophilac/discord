<Window x:Class="Discord_win.Dialog.CreateServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord_win.Dialog"
        mc:Ignorable="d"
        Title="Create a server" Height="138" Width="379" Closing="Window_Closing">
    <Grid>
        <Label Content="Server name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxServerName" HorizontalAlignment="Left" Height="23" Margin="91,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="260"/>
        <Button x:Name="buttonCreate" Content="Create" HorizontalAlignment="Left" Margin="276,56,0,0" VerticalAlignment="Top" Width="75" Click="ButtonCreate_Click"/>

    </Grid>
</Window>
